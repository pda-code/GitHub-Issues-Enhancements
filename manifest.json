{
  "manifest_version": 2,
  "name": "GitHub Issues Enhancements",
  "version": "1.0",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage","activeTab"],
  "content_scripts": [
    {
      "matches": ["*://github.com/*/*/issues*","*://github.com/*/*/labels/*","*://github.com/*/*/milestones/*"],
      "js": ["jquery-3.5.1.min.js","contentScript.js"]
    }
  ]
}